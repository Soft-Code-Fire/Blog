---
import Layout from "../layout/Layout.astro";
---

<Layout title='Blog'>
    <section>
        <div class="container px-6 py-10 mx-auto">
            <h1 class="text-2xl font-semibold text-gray-800 capitalize lg:text-3xl dark:text-white">From the blog</h1>

            <div class="grid grid-cols-1 gap-8 mt-8 md:mt-16 md:grid-cols-2">
                <div class="lg:flex">
                    <img class="object-cover w-full h-56 rounded-lg lg:w-64" src="https://images.unsplash.com/photo-1515378960530-7c0da6231fb1?ixlib=rb-1.2.1&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=1470&q=80" alt="">

                    <div class="flex flex-col justify-between py-6 lg:mx-6">
                        <a href="/blog/posts/welcome" class="text-xl font-semibold text-gray-800 hover:underline dark:text-white ">
                            Welcome to my blog
                        </a>

                        <span class="text-sm text-gray-500 dark:text-gray-300">4/5/2023</span>
                    </div>
                </div>
            </div>
            <div class="grid grid-cols-1 gap-8 mt-8 md:mt-16 md:grid-cols-2">
                <div class="lg:flex">
                    <img class="object-cover w-full h-56 rounded-lg lg:w-64" src="data:image/png;base64," alt="A8">

                    <div class="flex flex-col justify-between py-6 lg:mx-6">
                        <a href="/blog/posts/a8" class="text-xl font-semibold text-gray-800 hover:underline dark:text-white ">
                            A8 Stack
                        </a>

                        <span class="text-sm text-gray-500 dark:text-gray-300">5/29/2023</span>
                    </div>
                </div>
            </div>
        </div>
    </section>
</Layout>
